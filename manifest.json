{
	"manifest_version": 2,

	"name": "pydocs version loader",
	"description": "An extension to load your prefered version of the python docs",
	"version": "0.3.0",
	"permissions": [
		"storage",
		"webNavigation"
	],

	"options_ui": {
		"page": "options.html",
		"open_in_tab": false
	},

	"background": {
		"scripts": ["background.js"],
		"persistent": false
	}
}
